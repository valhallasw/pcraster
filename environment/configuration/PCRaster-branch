# vim:syntax=sh

_personalConfiguration="$PROJECTS/`ls $PROJECTS | grep -i \"^devenv$\"`/configuration/personal/kor"
source "$_personalConfiguration/profiles/common" $1
source "$_personalConfiguration/profiles/DevEnv"
source "$_personalConfiguration/profiles/RasterFormat"
source "$_personalConfiguration/profiles/Xsd"
source "$_personalConfiguration/profiles/Dal"
source "$_personalConfiguration/profiles/Aguila"
source "$_personalConfiguration/profiles/PcrTree2"
source "$_personalConfiguration/profiles/DataAssimilation"
unset _personalConfiguration

_cwd_branch=$(cd "$(dirname "${BASH_SOURCE[0]}" )" && pwd)
source "$_cwd_branch/PCRaster"
unset _cwd_branch

cd $PCRASTER
setPromptForProject PCRaster $PCRSTER_BRANCH $BUILD_TYPE
